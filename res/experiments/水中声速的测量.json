{
  "meta": {
    "name": "水中声速的测量",
    "author": "117503445",
    "contributors": ["小柴"],
    "version": "0.0.1",
    "LastEditTime": "2022-05-16 00:20"
  },
  "logic": {
    "functions": {
      "get_λ": "(function(arr){var arr_r = [];for (var i = 0; i < 5; i++) {arr_r.push((parseFloat(arr[i+5]) - parseFloat(arr[i]))*0.4);}return arr_r;})"
    },
    "variables": [
      {
        "name": "t",
        "comment": "室温",
        "source": {
          "type": "input",
          "default": 27.9
        }
      },
      {
        "name": "T",
        "comment": "开尔文温度",
        "source": {
          "type": "mathjs",
          "expression": "273.15 + t"
        }
      },
      {
        "name": "v_0",
        "comment": "室温理论速度",
        "source": {
          "type": "mathjs",
          "expression": "1557-0.0245*(75-t)^2"
        }
      },
      {
        "name": "f",
        "comment": "频率",
        "source": {
          "type": "input",
          "default": 37
        }
      },
      {
        "name": "f_d_yiqi",
        "comment": "频率不确定度(仪器) Δ𝑓",
        "source": {
          "type": "input",
          "default": 0.037
        }
      },
      {
        "name": "l_d_yiqi",
        "comment": "距离不确定度(仪器) ΔL",
        "source": {
          "type": "input",
          "default": 0.02
        }
      },
      {
        "name": "l",
        "comment": "距离",
        "source": {
          "type": "input",
          "default": [
            16.93,
            42.24,
            70.11,
            98.23,
            120.85,
            145.73,
            168.79,
            192.09,
            215.76,
            238.41
          ]
        }
      },
      {
        "name": "λ",
        "comment": "波长",
        "source": {
          "type": "mathjs",
          "expression": "get_λ(l)"
        }
      },
      {
        "name": "λ_a",
        "comment": "波长平均值",
        "source": {
          "type": "mathjs-suffix"
        }
      },
      {
        "name": "λ_u",
        "comment": "波长不确定度",
        "source": {
          "type": "mathjs",
          "expression": "smartlab_u(λ,0.02)"
        }
      },
      {
        "name": "v",
        "comment": "平均声速",
        "source": {
          "type": "mathjs",
          "expression": "λ_a * f"
        }
      },
      {
        "name": "v_u_re",
        "comment": "声速相对不确定度",
        "source": {
          "type": "mathjs",
          "expression": "sqrt( (f_d_yiqi/f)^2 + (λ_u/λ_a)^2 )"
        }
      },
      {
        "name": "v_d",
        "comment": "声速误差",
        "source": {
          "type": "mathjs",
          "expression": "v * v_u_re"
        }
      },
      {
        "name": "v_result",
        "comment": "声速实验结果",
        "source": {
          "type": "mathjs",
          "expression": "concat(string(round(v,3)),\"±\",string(round(v_d,2)))"
        }
      },
      {
        "name": "v_d_re",
        "comment": "声速相对误差",
        "source": {
          "type": "mathjs",
          "expression": "abs(v_0-v)/v_0*100"
        }
      }
    ]
  },

{
  "meta": {
    "name": "薄透镜焦距的测量",
    "author": "117503445",
    "contributors": ["尘与风"],
    "version": "0.0.1",
    "LastEditTime": "2021-10-29 21:13"
  },
  "logic": {
    "functions": {},
    "variables": [
      {
        "name": "F_1",
        "comment": "透镜焦距",
        "source": {
          "type": "input",
          "default": [15.2, 15.05, 15.16, 15.09, 15.0, 15.05, 15.1, 15.1]
        }
      },
      {
        "name": "F_1_a",
        "comment": "F_1 平均值",
        "source": {
          "type": "mathjs-suffix"
        }
      },
      {
        "name": "F_1_ua",
        "comment": "F_1 A类不确定度",
        "source": {
          "type": "mathjs-suffix"
        }
      },
      {
        "name": "F_1_u",
        "comment": "F_1 不确定度",
        "source": {
          "type": "mathjs",
          "expression": "smartlab_u(F_1,0.05)"
        }
      },
      {
        "name": "F_1_result",
        "comment": "F_1 结果",
        "source": {
          "type": "mathjs",
          "expression": "concat(string(round(F_1_a,2)),\"±\",string(round(F_1_u,2)))"
        }
      },
      {
        "name": "X_0",
        "comment": "物屏位置",
        "source": {
          "type": "input",
          "default": [-15, -18, -20]
        }
      },
      {
        "name": "X_3",
        "comment": "像屏位置",
        "source": {
          "type": "input",
          "default": [50, 50, 50]
        }
      },
      {
        "name": "X_1",
        "comment": "透镜位置",
        "source": {
          "type": "input",
          "default": [13.5, 14.0, 13.55]
        }
      },
      {
        "name": "X_2",
        "comment": "透镜位置",
        "source": {
          "type": "input",
          "default": [18.1, 18.01, 18.01]
        }
      },
      {
        "name": "D",
        "comment": "固定物与像屏的间距",
        "source": {
          "type": "mathjs",
          "expression": "abs(X_3-X_0)"
        }
      },
      {
        "name": "d",
        "comment": "两次成像时的位移",
        "source": {
          "type": "mathjs",
          "expression": "abs(X_2-X_1)"
        }
      },
      {
        "name": "F_2",
        "comment": "透镜焦距",
        "source": {
          "type": "mathjs",
          "expression": "(D.^2-d.^2)./(4*D)"
        }
      },
      {
        "name": "F_2_a",
        "comment": "透镜焦距 平均值",
        "source": {
          "type": "mathjs-suffix"
        }
      },
      {
        "name": "X_4",
        "comment": "凸透镜的位置读数",
        "source": {
          "type": "input",
          "default": [16.5, 16.49, 16.48, 16.51, 16.5, 16.5, 16.5, 16.51]
        }
      },
      {
        "name": "X_5",
        "comment": "F点的位置读数",
        "source": {
          "type": "input",
          "default": [26, 26.35, 26, 26.01, 26.02, 26.2, 26.3, 26.25]
        }
      },
      {
        "name": "F_3",
        "comment": "透镜焦距",
        "source": {
          "type": "mathjs",
          "expression": "-abs(X_5-X_4)"
        }
      },
      {
        "name": "F_3_a",
        "comment": "透镜焦距 平均值",
        "source": {
          "type": "mathjs-suffix"
        }
      },
      {
        "name": "F_3_u",
        "comment": "F_3 不确定度",
        "source": {
          "type": "mathjs",
          "expression": "smartlab_u(F_3,0.05)"
        }
      },
      {
        "name": "F_3_result",
        "comment": "F_3 结果",
        "source": {
          "type": "mathjs",
          "expression": "concat(string(round(F_3_a,2)),\"±\",string(round(F_3_u,2)))"
        }
      },
      {
        "name": "E_F_3",
        "comment": "相对不确定度",
        "source": {
          "type": "mathjs",
          "expression": "abs(F_3_u / F_3_a) * 100"
        }
      }
    ]
  },

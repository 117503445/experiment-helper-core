                "name": "I",
                "comment": "圆环转动惯量",
                "source": {
                    "type": "mathjs",
                    "expression": "smartlab_g * R_a * r_a * ((m_0+m)*T_a^2-m_0*T_0_a^2) * 100/ (4 * pi^2 * H_a)"
                }
            },
